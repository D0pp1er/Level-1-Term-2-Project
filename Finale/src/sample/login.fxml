<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="675.0" prefWidth="995.0" style="-fx-background-color: #88fff5;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="426.0" layoutY="154.0" text="Club Name" textAlignment="CENTER" textFill="#286269">
         <font>
            <Font name="Bodoni MT Poster Compressed" size="75.0" />
         </font></Label>
      <TextField fx:id="userText" layoutX="358.0" layoutY="288.0" prefHeight="70.0" prefWidth="319.0" style="-fx-background-color: #45d5fd; -fx-alignment: Center;">
         <font>
            <Font name="Bell MT" size="30.0" />
         </font></TextField>
    <Button fx:id="resetButton" layoutX="371.0" layoutY="420.0" mnemonicParsing="false" onAction="#resetAction" prefHeight="47.0" prefWidth="110.0" style="-fx-background-color: #a745fd;" text="Reset" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Berlin Sans FB" size="30.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="545.0" layoutY="420.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="47.0" prefWidth="129.0" style="-fx-background-color: #a745fd;" text="Log In" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Berlin Sans FB" size="30.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
