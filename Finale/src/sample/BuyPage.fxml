<AnchorPane prefHeight="675.0" prefWidth="995.0" style="-fx-background-color: #03dac6;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BuyPageController">
   <children>
      <Label layoutX="190.0" layoutY="-2.0" prefHeight="84.0" prefWidth="806.0" style="-fx-alignment: Center; -fx-background-color: #03dac6;" text="Transfer Market" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Calisto MT" size="60.0" />
         </font>
      </Label>
      <TableView fx:id="BuyPageTable" layoutX="-10.0" layoutY="90.0" prefHeight="480.0" prefWidth="1006.0" style="-fx-background-color: #5382a7;">
         <effect>
            <Glow />
         </effect></TableView>
      <Label fx:id="tableTitle" layoutY="-2.0" prefHeight="84.0" prefWidth="190.0" style="-fx-alignment: center; -fx-background-color: #03dac6;" text="Welcome, Manchester United" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Calisto MT" size="20.0" />
         </font>
      </Label>
      <Button layoutX="190.0" layoutY="585.0" mnemonicParsing="false" onAction="#backtoHomePage" prefHeight="75.0" prefWidth="168.0" style="-fx-alignment: center; -fx-background-radius: 15; -fx-background-color: #ffffff;" text="Back" textFill="#f42424">
         <font>
            <Font name="Bell MT Bold" size="40.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="601.0" layoutY="585.0" mnemonicParsing="false" onAction="#BuyPlayer" prefHeight="75.0" prefWidth="168.0" style="-fx-alignment: center; -fx-background-radius: 15; -fx-background-color: #ffffff;" text="Buy" textFill="#00cfaf">
         <font>
            <Font name="Bell MT Bold" size="40.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
